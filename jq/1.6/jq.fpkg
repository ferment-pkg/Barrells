pkgname=jq
version=1.6
desc="Lightweight and flexible command-line JSON processor"
arch="universal"
source="https://github.com/stedolan/jq/releases/download/jq-1.6/jq-1.6.tar.gz"
license="MIT"
Dbuild=autoconf,automake
build(){
    ./configure --prefix=usr/local --disable-dependency-tracking --disable-silent-rules --disable-maintainer-mode
    make -j4
}

test(){
    @output="command -v jq"
    match $output != ""
}
install(){
    make install
}